{
  "name": "@omnilert/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate:master": "knex migrate:latest --knexfile knexfile.ts --env master",
    "migrate:master:rollback": "knex migrate:rollback --knexfile knexfile.ts --env master",
    "migrate:tenant": "tsx src/scripts/migrate-tenants.ts",
    "migrate:tenant:status": "tsx src/scripts/migration-status-tenants.ts",
    "migrate:tenant:rollback": "tsx src/scripts/rollback-tenants.ts",
    "seed:master": "knex seed:run --knexfile knexfile.ts --env master",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.650.0",
    "@omnilert/shared": "workspace:*",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.0",
    "pg-boss": "^12.13.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "socket.io": "^4.8.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "rimraf": "^6.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
